<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Optionen</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.usersPluginUid>
                        <TCEforms>
                            <label>LLL:EXT:projectmanager/Resources/Private/Language/locallang_db.xlf:usersPluginUid</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>20</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.usersPluginUid>  
                    <settings.businessesPluginUid>
                        <TCEforms>
                            <label>LLL:EXT:projectmanager/Resources/Private/Language/locallang_db.xlf:businessesPluginUid</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>20</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.businessesPluginUid> 
                    <settings.projectsPluginUid>
                        <TCEforms>
                            <label>LLL:EXT:projectmanager/Resources/Private/Language/locallang_db.xlf:projectsPluginUid</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>20</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.projectsPluginUid>                                                               
                    <settings.tasksPluginUid>
                        <TCEforms>
                            <label>LLL:EXT:projectmanager/Resources/Private/Language/locallang_db.xlf:tasksPluginUid</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>20</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.tasksPluginUid>   
                    <settings.timesPluginUid>
                        <TCEforms>
                            <label>LLL:EXT:projectmanager/Resources/Private/Language/locallang_db.xlf:timesPluginUid</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <max>20</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.timesPluginUid>                                          
                    <settings.categoryParent>
                        <TCEforms>
                            <label>LLL:EXT:projectmanager/Resources/Private/Language/locallang_db.xlf:category-parent</label>
                            <config>
                                <type>select</type>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:projectmanager/Resources/Private/Language/locallang_db.xlf:category-parent.please-select</numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>       
                                <foreign_table>sys_category</foreign_table>
                                <foreign_table_where> ORDER BY sys_category.title ASC</foreign_table_where>
                                <size>1</size>
                                <min>1</min>
                                <max>1</max>
                            </config>
                        </TCEforms>
                    </settings.categoryParent>                                                             
                    <settings.pagination>
                        <TCEforms>
                            <label>LLL:EXT:projectmanager/Resources/Private/Language/locallang_db.xlf:pagination</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>check</type>
                            </config>                            
                        </TCEforms>
                    </settings.pagination>                      
                    <settings.paginationItems>
                        <TCEforms>
                            <label>LLL:EXT:projectmanager/Resources/Private/Language/locallang_db.xlf:items-per-page</label>
                            <displayCond>FIELD:settings.pagination:REQ:TRUE</displayCond>
                            <config>
                                <type>input</type>
                                <size>20</size>
                                <max>30</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.paginationItems>  
                    <settings.paginationLinks>
                        <TCEforms>
                            <label>LLL:EXT:projectmanager/Resources/Private/Language/locallang_db.xlf:number-of-pagination-links</label>
                            <displayCond>FIELD:settings.pagination:REQ:TRUE</displayCond>
                            <config>
                                <type>input</type>
                                <size>20</size>
                                <max>30</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.paginationLinks>                                                          
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
